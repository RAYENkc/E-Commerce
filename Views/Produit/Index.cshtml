@model IEnumerable<TP_Ecommerce.Models.produit>
@{
    ViewBag.Title = "Index";
} 
<h2>Index</h2>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Categorie.NameCat)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Marque.NameMrq)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProductName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.qte)
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Categorie.NameCat)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Marque.NameMrq)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProductName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Price)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.qte)
            </td>
        </tr>
    }
</table>
